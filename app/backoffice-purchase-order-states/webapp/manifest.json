{
	"_version": "1.29.0",
	"sap.app": {
		"id": "ch.amag.retail.dwb.backoffice-purchase-order-states",
		"type": "application",
		"i18n": {
			"bundleUrl": "i18n/i18n.properties",
			"supportedLocales": [
				"de",
				"it",
				"fr",
				"en"
			],
			"fallbackLocale": "de"
		},
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{AppTitle}}",
		"description": "{{AppSubtitle}}",
		"dataSources": {
			"mainService": {
				"uri": "/app/backend/backoffice/purchase-order-states",
				"type": "OData",
				"settings": {
					"odataVersion": "4.0"
				}
			}
		},
		"crossNavigation": {
			"inbounds": {
				"DWBBOPurchaseOrderState-manage": {
					"signature": {
						"parameters": {},
						"additionalParameters": "allowed"
					},
					"semanticObject": "DWBBOPurchaseOrderState",
					"action": "manage",
					"title": "{{AppTitle}}",
					"subTitle": "{{AppSubtitle}}",
					"icon": ""
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.102.1",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"sap.fe.templates": {},
				"sap.ui.integration": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
                    "bundleUrl": "i18n/i18n.properties",
                    "supportedLocales": [
                        "de",
                        "it",
                        "fr",
                        "en"
                    ],
                    "fallbackLocale": "de"
                }
			},			
			"": {
				"dataSource": "mainService",
				"preload": true,
				"settings": {
					"synchronizationMode": "None",
					"operationMode": "Server",
					"autoExpandSelect": true,
					"earlyRequests": true
				}
			}
		},
		"routing": {
			"routes": [
				{
                    "pattern": ":?query:",
                    "name": "PurchaseOrderStates",
                    "target": "PurchaseOrderStates"
                }
			],
			"targets": {
				"PurchaseOrderStates": {
                    "type": "Component",
                    "id": "PurchaseOrderStates",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings": {
                            "entitySet": "PurchaseOrderStates",
                            "variantManagement": "Page",
                            "initialLoad": true,
                            "navigation": {
                            }
                        }
                    },
                    "contextPattern": ""
                }
			},
			"config": {
				"flexibleColumnLayout": {
					"defaultTwoColumnLayoutType": "TwoColumnsBeginExpanded",
					"defaultThreeColumnLayoutType": "ThreeColumnsBeginExpanded"
				},
				"routerClass": "sap.f.routing.Router"
			}
		},
		"rootView": {
			"viewName": "sap.fe.templates.RootContainer.view.Fcl",
			"type": "XML",
			"async": true,
			"id": "appRootView"
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		}
	},
	"sap.cloud": {
		"public": true,
		"service": "ch.amag.retail.dwb"
	}
}