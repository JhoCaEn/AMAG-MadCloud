{
	"_version": "1.29.0",
	"sap.app": {
		"id": "ch.amag.retail.dwb.backoffice-engines",
		"type": "application",
		"i18n": {
			"bundleUrl": "i18n/i18n.properties",
			"supportedLocales": [
				"de",
				"it",
				"fr",
				"en"
			],
			"fallbackLocale": "en"
		},
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{AppTitle}}",
		"description": "{{AppSubtitle}}",
		"dataSources": {
			"mainService": {
				"uri": "app/backend/backoffice/engines/",
				"type": "OData",
				"settings": {
					"odataVersion": "4.0"
				}
			}
		},
		"crossNavigation": {
			"inbounds": {
				"DWBBOEngine-manage": {
					"signature": {
						"parameters": {},
						"additionalParameters": "allowed"
					},
					"semanticObject": "DWBBOEngine",
					"action": "manage",
					"title": "{{AppTitle}}",
					"subTitle": "{{AppSubtitle}}",
					"icon": ""
				}
			},
			"outbounds": {
				"Model": {
					"semanticObject": "DWBBOModel",
					"action": "manage"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.102.1",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"sap.fe.templates": {},
				"sap.ui.integration": {}
			}
		},
		"extends": {
			"extensions": {
				"sap.ui.controllerExtensions": {
					"sap.fe.templates.ObjectPage.ObjectPageController#ch.amag.retail.dwb.backoffice-engines::Engine": {
						"controllerName": "ch.amag.retail.dwb.backoffice-engines.controller.Engine"
					}
				}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
                    "bundleUrl": "i18n/i18n.properties",
                    "supportedLocales": ["de", "it", "fr", "en"],
                    "fallbackLocale": "de"
                }
			},
			"": {
				"dataSource": "mainService",
				"preload": true,
				"settings": {
					"synchronizationMode": "None",
					"operationMode": "Server",
					"autoExpandSelect": true,
					"earlyRequests": true
				}
			}
		},
		"routing": {
			"routes": [
				{
					"pattern": ":?query:",
					"name": "Engines",
					"target": "Engines"
				},
				{
					"pattern": "Engines({key}):?query:",
					"name": "Engine",
					"target": "Engine"
				}
			],
			"targets": {
				"Engines": {
					"type": "Component",
					"id": "Engines",
					"name": "sap.fe.templates.ListReport",
					"options": {
						"settings": {
							"entitySet": "Engines",
							"variantManagement": "Page",
							"initialLoad": true,
							"navigation": {
								"Engines": {
									"detail": {
										"route": "Engine"
									}
								}
							},
							"controlConfiguration": {
								"@com.sap.vocabularies.UI.v1.SelectionFields": {
									"showClearButton": true
								}
							}
						}
					},
					"contextPattern": ""
				},
				"Engine": {
					"type": "Component",
					"id": "Engine",
					"name": "sap.fe.templates.ObjectPage",
					"options": {
						"settings": {
							"sectionLayout": "Tabs",
							"entitySet": "Engines",
							"navigation": {
								"models": {
									"detail": {
										"path": "sap.apps.crossNavigation.outbounds",
										"outbound": "Model"
									}
								}
							}
						}
					},
					"contextPattern": "/Engines({key})"
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		}
	},
	"sap.cloud": {
		"public": true,
		"service": "ch.amag.retail.dwb"
	}
}