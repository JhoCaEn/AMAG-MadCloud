{
  "_version": "1.49.0",
  "sap.app": {
    "id": "ch.amag.retail.dwb.sessions",
    "type": "application",
    "i18n": {
			"bundleUrl": "i18n/i18n.properties",
			"supportedLocales": [
				"de",
				"it",
				"fr",
				"en"
			],
			"fallbackLocale": "en"
		},
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.9.6",
      "toolsId": "3d327915-4987-4a6a-bb1f-f8e6d9e1fe01"
    },
    "dataSources": {
      "mainService": {
        "uri": "/app/backend/sessions/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "4.0"
        }
      }
    },
		"crossNavigation": {
			"inbounds": {
				"DWBSession-manage": {
					"signature": {
						"parameters": {},
						"additionalParameters": "allowed"
					},
					"semanticObject": "DWBSession",
					"action": "manage",
					"title": "{{appTitle}}",
					"subTitle": "{{appSubTitle}}",
					"icon": ""
				}
			}
		}
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.115.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ushell": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "sessions.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "SessionsList",
          "target": "SessionsList"
        },
        {
          "pattern": "Sessions({key}):?query:",
          "name": "SessionsObjectPage",
          "target": "SessionsObjectPage"
        }
      ],
      "targets": {
        "SessionsList": {
          "type": "Component",
          "id": "SessionsList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "entitySet": "Sessions",
              "variantManagement": "None",
              "navigation": {
                "Sessions": {
                  "detail": {
                    "route": "SessionsObjectPage"
                  }
                }
              }
            }
          }
        },
        "SessionsObjectPage": {
          "type": "Component",
          "id": "SessionsObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "entitySet": "Sessions"
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
