{
  "_version": "1.29.0",
  "sap.app": {
    "id": "ch.amag.retail.dwb.backoffice-models",
    "type": "application",
    "i18n": {
      "bundleUrl": "i18n/i18n.properties",
      "supportedLocales": ["de", "it", "fr", "en"],
      "fallbackLocale": "de"
    },
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{AppTitle}}",
    "description": "{{AppSubtitle}}",
    "dataSources": {
      "mainService": {
        "uri": "app/backend/backoffice/models/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "DWBBOModel-manage": {
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          },
          "semanticObject": "DWBBOModel",
          "action": "manage",
          "title": "{{AppTitle}}",
          "subTitle": "{{AppSubtitle}}",
          "icon": ""
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "dependencies": {
      "minUI5Version": "1.102.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {},
        "sap.ui.integration": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleUrl": "i18n/i18n.properties",
          "supportedLocales": ["de", "it", "fr", "en"],
          "fallbackLocale": "de"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "routing": {
      "routes": [
        {
          "pattern": ":?query:",
          "name": "Models",
          "target": "Models"
        },
        {
          "pattern": "Models({key}):?query:",
          "name": "Model",
          "target": "Model"
        },

        {
          "pattern": "Models({key})/colors({key2}):?query:",
          "name": "ModelColors",
          "target": "ModelColors"
        },

        {
          "pattern": "Models({key})/restrictions({key2}):?query:",
          "name": "ModelRestriction",
          "target": "ModelRestriction"
        },
        {
          "pattern": "Models({key})/colorCombinations({key2}):?query:",
          "name": "ModelColorCombination",
          "target": "ModelColorCombination"
        },
        {
          "pattern": "Models({key})/equipments({key2}):?query:",
          "name": "ModelEquipment",
          "target": "ModelEquipment"
        },
        {
          "pattern": "Models({key})/colorCombinations({key2})/restrictions({key3}):?query:",
          "name": "ModelColorRestriction",
          "target": "ModelColorRestriction"
        },
        {
          "pattern": "Models({key})/colorCombinations({key2})/restrictions({key3})/options({key4}):?query:",
          "name": "ModelColorRestrictionOptions",
          "target": "ModelColorRestrictionOptions"
        },
        {
          "pattern": "Models({key})/equipments({key2})/restrictions({key3}):?query:",
          "name": "EquipmentRestriction",
          "target": "EquipmentRestriction"
        },
        {
          "pattern": "Models({key})/equipments({key2})/restrictions({key3})/options({key4}):?query:",
          "name": "EquipmentRestrictionOption",
          "target": "EquipmentRestrictionOption"
        }
      ],
      "targets": {
        "Models": {
          "type": "Component",
          "id": "Models",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "entitySet": "Models",
              "variantManagement": "Page",
              "initialLoad": true,
              "navigation": {
                "Models": {
                  "detail": {
                    "route": "Model"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.SelectionFields": {
                  "showClearButton": true
                }
              }
            }
          },
          "contextPattern": ""
        },
        "Model": {
          "type": "Component",
          "id": "Model",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "entitySet": "Models",
              "sectionLayout": "Tabs",
              "navigation": {
                "restrictions": {
                  "detail": {
                    "route": "ModelRestriction"
                  }
                },
                "colorCombinations": {
                  "detail": {
                    "route": "ModelColorCombination"
                  }
                },
                "colors": {
                  "detail": {
                    "route": "ModelColors"
                  }
                },
                "equipments": {
                  "detail": {
                    "route": "ModelEquipment"
                  }
                }
              }
            }
          },
          "contextPattern": "/Models({key})"
        },
        "ModelColors": {
          "type": "Component",
          "id": "ModelColors",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "entitySet": "ModelColors"
            }
          },
          "contextPattern": "/Models({key})/colors({key2})"
        },
        "ModelRestriction": {
          "type": "Component",
          "id": "ModelRestriction",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "entitySet": "ModelRestrictions",
              "sectionLayout": "Tabs"
            }
          },
          "contextPattern": "/Models({key})/restrictions({key2})"
        },
        "ModelColorCombination": {
          "type": "Component",
          "id": "ModelColorCombination",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "entitySet": "ModelColorCombinations",
              "navigation": {
                "restrictions": {
                  "detail": {
                    "route": "ModelColorRestriction"
                  }
                }
              }
            }
          },
          "contextPattern": "/Models({key})/colorCombinations({key2})"
        },
        "ModelEquipment": {
          "type": "Component",
          "id": "ModelEquipment",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "entitySet": "ModelEquipments",
              "sectionLayout": "Tabs",
              "navigation": {
                "restrictions": {
                  "detail": {
                    "route": "EquipmentRestriction"
                  }
                }
              }
            }
          },
          "contextPattern": "/Models({key})/equipments({key2})"
        },
        "ModelColorRestriction": {
          "type": "Component",
          "id": "ModelColorRestriction",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "entitySet": "ModelColorRestrictions",
              "navigation": {
                "options": {
                  "detail": {
                    "route": "ModelColorRestrictionOptions"
                  }
                }
              }
            }
          },
          "contextPattern": "/Models({key})/colorCombinations({key2})/restrictions({key3})"
        },
        "ModelColorRestrictionOptions": {
          "type": "Component",
          "id": "ModelColorRestrictionOptions",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "entitySet": "ModelColorRestrictionOptions"
            }
          },
          "contextPattern": "/Models({key})/colorCombinations({key2})/restrictions({key3})/options({key4})"
        },
        "EquipmentRestriction": {
          "type": "Component",
          "id": "EquipmentRestriction",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "sectionLayout": "Tabs",
              "entitySet": "ModelEquipmentRestrictions",
              "navigation": {
                "options": {
                  "detail": {
                    "route": "EquipmentRestrictionOption"
                  }
                }
              }
            }
          },
          "contextPattern": "/Models({key})/equipments({key2})/restrictions({key3})"
        },
        "EquipmentRestrictionOption": {
          "type": "Component",
          "id": "EquipmentRestrictionOption",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "entitySet": "ModelEquipmentRestrictionOptions"
            }
          },
          "contextPattern": "/Models({key})/equipments({key2})/restrictions({key3})/options({key4})"
        }
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "ch.amag.retail.dwb"
  }
}
